{
  "name": "@naiheyoung/open-cli",
  "version": "1.0.2",
  "packageManager": "pnpm@10.13.1",
  "description": "find local files and open them.",
  "homepage": "https://github.com/naiheyoung/open-cli#readme",
  "author": "Naihe",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.js",
      "require": "./dist/cli.cjs"
    }
  },
  "bin": {
    "open": "bin/cli.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run clean && tsup"
  },
  "keywords": [
    "file search",
    "open",
    "windows",
    "esm",
    "cjs",
    "cli"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/prompts": "^2.4.9",
    "cac": "^6.7.14",
    "chalk": "^5.5.0",
    "ora": "^8.2.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "prompts": "^2.4.2"
  }
}
